#!/usr/bin/env python


import nne
import argparse

parser = argparse.ArgumentParser(description='Neural Network Graph Analyzer')
parser.add_argument("model_path", type=str, help="model path for analizing")
parser.add_argument("-o", "--output", type=str, help="Specify the path to output the Node information of the model in json format.")
args = parser.parse_args()
nne.analyze(args.model_path, args.output)
